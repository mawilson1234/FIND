{
    "arch": ["transformer"],
    "decoder-attention-heads": [8],
    "encoder-attention-heads": [8],
    "encoder-embed-dim": [64],
    "encoder-ffn-embed-dim": [256],
    "encoder-layers": [4],
    "decoder-embed-dim": [64],
    "decoder-ffn-embed-dim": [256],
    "decoder-layers":  [4],
    "lr": [1e-3],
    "dropout": [0.5],
    "lr-scheduler": ["inverse_sqrt"],
    "warmup-init-lr": [1e-5],
    "min-lr": [1e-9],
    "warmup-updates" : [1000],
    "mdl-epochs": [500],
    "mdl-batch-size": [256],
	"mdl-batches-per-epoch": [1000],
    "optimizer": ["adam"],
    "seed": [5]
}
